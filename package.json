{
  "name": "incan-gold",
  "version": "0.0.1",
  "description": "Server for playing Incan Gold",
  "engines": {
    "node": "~6.0.0",
    "npm": "~3.8.9"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "http://www.example.com"
  },
  "keywords": [
    "incan",
    "gold",
    "game"
  ],
  "author": "Andrew Huff",
  "license": "MIT",
  "main": "app.js",
  "scripts": {
    "devserver": "nodemon app.js",
    "lint": "eslint app.js api/ test/",
    "lint-fix": "eslint --fix app.js api/ test/",
    "test": "mocha 'test/**/*.js' --compilers js:babel-core/register"
  },
  "dependencies": {
    "babel-core": "6.7.4",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "body-parser": "1.15.2",
    "express": "4.13.4",
    "express-healthcheck": "0.1.0",
    "lodash": "4.13.1"
  },
  "devDependencies": {
    "babel-eslint": "6.0.2",
    "chai": "3.5.0",
    "eslint": "2.7.0",
    "eslint-plugin-lodash": "1.9.2",
    "eslint-plugin-mocha": "3.0.0",
    "mocha": "2.5.3",
    "nodemon": "1.9.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "browser": false,
      "node": true,
      "mocha": true
    },
    "globals": {
      "Promise": false
    },
    "extends": [
      "eslint:recommended",
      "plugin:lodash/recommended"
    ],
    "plugins": [
      "mocha",
      "lodash"
    ],
    "rules": {
      "consistent-return": "error",
      "mocha/handle-done-callback": "error",
      "mocha/no-exclusive-tests": "error",
      "mocha/no-global-tests": "error",
      "mocha/no-pending-tests": "error",
      "mocha/no-skipped-tests": "error",
      "mocha/no-synchronous-tests": "off",
      "no-console": "off",
      "no-unexpected-multiline": "error",
      "no-unused-vars": [
        "error",
        {
          "args": "none"
        }
      ],
      "semi": [
        "error",
        "always"
      ],
      "strict": "off",
      "wrap-iife": [
        "error",
        "any"
      ]
    }
  }
}
